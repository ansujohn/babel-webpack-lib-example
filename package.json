  "main": "dist/sampleLib.js",
  "module": "src/index.js",
  "description": "An ES6 sample project using Babel and Webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ansujohn/babel-webpack-lib-example"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "babel",
    "webpack",
    "es6",
