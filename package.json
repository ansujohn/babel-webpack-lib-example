    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "mocha": "^3.2.0",
    "standard-version": "^4.0.0",
    "webpack": "2.4.1",
